<?xml version="1.0" encoding="UTF-8"?><web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"	version="2.4">	<!-- Standard Action Servlet Configuration (with debugging) -->	<servlet>		<servlet-name>action</servlet-name>		<servlet-class>org.apache.struts.action.ActionServlet</servlet-class>		<init-param>			<param-name>config</param-name>			<param-value>/WEB-INF/struts-config.xml</param-value>		</init-param>		<init-param>			<param-name>debug</param-name>			<param-value>2</param-value>		</init-param>		<init-param>			<param-name>detail</param-name>			<param-value>2</param-value>		</init-param>		<load-on-startup>2</load-on-startup>	</servlet>	<session-config>	<session-timeout>60</session-timeout>	</session-config>	<!-- Standard Action Servlet Mapping -->	<servlet-mapping>		<servlet-name>action</servlet-name>		<url-pattern>*.do</url-pattern>	</servlet-mapping>	 <filter>		<filter-name>companyFilter</filter-name>		<filter-class>			com.kaldin.company.filter.CompanyFilter		</filter-class>	</filter>	<filter-mapping>		<filter-name>companyFilter</filter-name>		<url-pattern>*.jsp</url-pattern>	</filter-mapping>	<filter-mapping>		<filter-name>companyFilter</filter-name>		<url-pattern>*.do</url-pattern>	</filter-mapping>	<servlet> 	<servlet-name>OpenIdServlet</servlet-name> 	<servlet-class>com.kaldin.openid.OpenIdServlet</servlet-class> 	<init-param> 	    <param-name>realm</param-name> 	    <param-value>http://www.ganib.com/otest</param-value> 	</init-param> 	<init-param> 	    <param-name>return_to_path</param-name> 	    <param-value>/openid</param-value> 	</init-param> 	<init-param> 	    <param-name>home_path</param-name> 	    <param-value>/otest/openId/openidregister.jsp</param-value> 	</init-param> 	</servlet> 	<servlet-mapping> 		<servlet-name>OpenIdServlet</servlet-name> 		<url-pattern>/openid</url-pattern> 	</servlet-mapping> 		<servlet>           <servlet-name>feedback</servlet-name>           <servlet-class>com.kaldin.feedback.controller.FeedBackServlet</servlet-class>    </servlet>    <servlet-mapping>           <servlet-name>feedback</servlet-name>           <url-pattern>/feedback</url-pattern>    </servlet-mapping>         <servlet>           <servlet-name>imgupload</servlet-name>           <servlet-class>com.kaldin.common.util.Imgupload</servlet-class>    </servlet>    <servlet-mapping>           <servlet-name>imgupload</servlet-name>           <url-pattern>/imgupload</url-pattern>    </servlet-mapping>		<servlet>    	<servlet-name>practiceExam</servlet-name>    	<servlet-class>com.kaldin.practice.servlet.PracticeExam</servlet-class>    </servlet>    <servlet-mapping>    	<servlet-name>practiceExam</servlet-name>    	<url-pattern>/practice-exam/*</url-pattern>    </servlet-mapping>	<!-- The Usual Welcome File List -->	<welcome-file-list>		<welcome-file>/jsp/common/index.jsp</welcome-file>	</welcome-file-list> 	<filter>		<filter-name>User Login Filter</filter-name>		<filter-class>com.kaldin.common.util.UserSessionFilter</filter-class>	</filter>	<filter-mapping>		<filter-name>User Login Filter</filter-name>		<url-pattern>*.jsp</url-pattern>	</filter-mapping>	<filter-mapping>		<filter-name>User Login Filter</filter-name>		<url-pattern>*.do</url-pattern>	</filter-mapping>	<filter-mapping>		<filter-name>User Login Filter</filter-name>		<url-pattern>/exam/*</url-pattern>	</filter-mapping>	<env-entry>	    <env-entry-name>DebugLevel</env-entry-name>	    <env-entry-type>java.lang.String</env-entry-type>	    <env-entry-value>true</env-entry-value>	</env-entry>	<env-entry>	    <env-entry-name>Info</env-entry-name>	    <env-entry-type>java.lang.String</env-entry-type>	    <env-entry-value>false</env-entry-value>	</env-entry>	<env-entry>	    <env-entry-name>Level3</env-entry-name>	    <env-entry-type>java.lang.String</env-entry-type>	    <env-entry-value>true</env-entry-value>	</env-entry>		<env-entry>		<env-entry-name>DataSourceName</env-entry-name>				<env-entry-type>java.lang.String</env-entry-type>		<env-entry-value>kaldin</env-entry-value>	</env-entry>		<env-entry>		<env-entry-name>license</env-entry-name>				<env-entry-type>java.lang.String</env-entry-type>		<env-entry-value>open-source</env-entry-value>		<!-- Types:  open-source, open-source-commercial, saas -->	</env-entry>	</web-app>













