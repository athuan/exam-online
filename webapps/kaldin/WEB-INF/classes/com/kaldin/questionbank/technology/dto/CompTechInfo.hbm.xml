<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kaldin.questionbank.technology.dto.CompTechInfoDTO" table="exm_company_technology">
		<meta attribute="class-description">
			This is the HiBernate Mapping file for exam class
		</meta>
		
		<id name="ctechId" column="ctechid" type="int">
			<generator class="native"></generator>
		</id>
		<property name="companyId" column="companyid" type="int"
			not-null="true">
		</property>
		<property name="technology" column="technology" type="string"
			not-null="true">
		</property>
	</class>
</hibernate-mapping>