<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kaldin.company.register.dto.CompanyDTO" table="exm_company">
		<meta attribute="class-description">
			This is the HiBernate Mapping file for Answer Table
		</meta>
		<id name="companyId" column="companyid" type="int">
			<generator class="native"></generator>
		</id>
		<property name="companyName" column="companyname" type="string"
			not-null="true">
		</property>
		<property name="companyUrl" column="companyurl" type="string"
			not-null="true">
		</property>
		<property name="isActive" column="isactive" type="int">
		</property>
		<property name="isDeleted" column="isdeleted" type="int">
		</property>
		<property name="isProfile" column="isprofile" type="int">
		</property>
	</class>
</hibernate-mapping>