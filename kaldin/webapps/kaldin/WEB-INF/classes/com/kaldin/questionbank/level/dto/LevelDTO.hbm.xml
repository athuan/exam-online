<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.kaldin.questionbank.level.dto.LevelDTO" table="exm_level">
		<id name="levelid" type="int" column="levelid">
			<generator class="native"></generator>             
		</id> 
		<property name="level" column="level" type="string">
		</property>
		<property name="companyId" column="companyid" type="int"
			not-null="true">
		</property>
	</class>
</hibernate-mapping>